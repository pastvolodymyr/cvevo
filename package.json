{
  "name": "cvevo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:local && conc npm:dev:next npm:dev:proxy",
    "dev:next": "next dev",
    "dev:proxy": "npx cf-bindings-proxy",
    "migrate:local": "npm run migrate -- --local",
    "migrate": "npx wrangler d1 migrations apply cvevo_db",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@auth/d1-adapter": "^1.4.1",
    "@cloudflare/next-on-pages": "^1.12.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@reduxjs/toolkit": "^2.2.6",
    "classnames": "^2.5.1",
    "jsonrepair": "^3.8.0",
    "next": "14.2.4",
    "next-auth": "^5.0.0-beta.19",
    "openai": "^4.52.7",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^9.1.2",
    "react-turnstile": "^1.1.3",
    "wrangler": "^3.63.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240620.0",
    "@miniflare/d1": "^2.14.2",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "sass": "^1.77.6",
    "scss": "^0.2.4"
  }
}
